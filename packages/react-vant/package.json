{
  "name": "react-vant",
  "version": "2.0.0-alpha.39",
  "description": "React Mobile UI Components base on Vant UI",
  "keywords": [
    "ui",
    "react",
    "mobile",
    "components",
    "component",
    "design",
    "framework",
    "frontend",
    "react-component"
  ],
  "homepage": "https://react-vant.3lang.dev",
  "repository": {
    "type": "git",
    "url": "git@github.com:3lang3/react-vant.git",
    "branch": "main",
    "package": "./packages/react-vant"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/react-vant"
  },
  "license": "MIT",
  "author": "3lang3",
  "sideEffects": [
    "**/*.css",
    "**/*.less",
    "./es/index.js",
    "./src/index.ts",
    "./es/styles/index.js",
    "./src/styles/index.ts"
  ],
  "exports": {
    ".": {
      "node": {
        "import": "./bundle/react-vant.es.js",
        "require": "./bundle/react-vant.js"
      },
      "require": "./lib/index.js",
      "import": "./es/index.js",
      "types": "./es/index.d.ts"
    },
    "./es": "./es/index.js",
    "./lib": "./lib/index.js",
    "./es/": "./es/",
    "./lib/": "./lib/",
    "./bundle/": "./bundle/",
    "./2x": "./2x/es/index.js",
    "./2x/": "./2x/",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "./lib/index.js",
  "jsdelivr": "./bundle/react-vant.min.js",
  "unpkg": "./bundle/react-vant.min.js",
  "module": "./es/index.js",
  "typings": "./es/index.d.ts",
  "style": "./bundle/index.css",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "rcdoc build && pnpm build:gulp",
    "build:gulp": "gulp",
    "deploy:gh-pages": "gh-pages -d docs-dist",
    "dev": "rcdoc dev",
    "docs:build": "rcdoc docs-build",
    "lint": "eslint --fix",
    "lint:tsx": "eslint src --ext ts,tsx",
    "prettier": "npx prettier --write vant.config.js src/index.ts",
    "preview": "rcdoc preview",
    "release": "pnpm publish ./dist --tag next",
    "release:dev": "pnpm publish ./dist --tag dev"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 8"
  ],
  "dependencies": {
    "@react-spring/web": "^9.3.0",
    "@react-vant/icons": "workspace:*",
    "@use-gesture/react": "^10.1.1",
    "@vant/popperjs": "^1.1.0",
    "clsx": "1.1.1",
    "rc-field-form": "^1.26.4",
    "react-transition-group": "^4.4.1",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@rcdoc/cli": "0.1.21",
    "@rcdoc/theme": "^0.1.6",
    "@types/node": "^17.0.31",
    "@types/react-dom": "^18.0.3",
    "@vant/area-data": "1.2.4",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.1",
    "postcss-px-multiple": "^0.1.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "through2": "^4.0.2",
    "typescript": "4.6.4"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
