{
  "name": "react-vant",
  "version": "2.0.0-dev.30-3",
  "description": "React Mobile UI Components base on Vant UI",
  "keywords": [
    "ui",
    "react",
    "mobile",
    "components",
    "component",
    "design",
    "framework",
    "frontend",
    "react-component"
  ],
  "homepage": "https://3lang3.github.io/react-vant",
  "repository": {
    "type": "git",
    "url": "git@github.com:3lang3/react-vant.git"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/react-vant"
  },
  "license": "MIT",
  "author": "3lang3",
  "sideEffects": [
    "**/*.css",
    "**/*.less",
    "./es/index.js",
    "./src/index.ts",
    "./es/styles/index.js",
    "./src/styles/index.ts"
  ],
  "main": "./lib/index.js",
  "jsdelivr": "./lib/react-vant.min.js",
  "unpkg": "./lib/react-vant.min.js",
  "module": "./es/index.js",
  "style": "./lib/index.css",
  "typings": "./es/index.d.ts",
  "exports": {
    ".": {
      "node": {
        "import": "./lib/react-vant.es.js",
        "require": "./lib/react-vant.cjs.js"
      },
      "import": "./es/index.js",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./es": "./es/index.js",
    "./lib": "./lib/index.js",
    "./es/": "./es/",
    "./es/*": "./es/*/index.js",
    "./lib/": "./lib/",
    "./2x": "./2x",
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "es",
    "2x"
  ],
  "scripts": {
    "build": "react-vant-cli build",
    "build:site": "react-vant-cli build-site",
    "changelog": "react-vant-cli changelog",
    "create-component": "react-vant-cli create-component",
    "postcreate-component": "yarn prettier",
    "prepack": "yarn build",
    "prettier": "npx prettier --write vant.config.js src/index.ts",
    "release": "pnpm publish --tag next",
    "release:dev": "pnpm publish --tag dev",
    "site:deploy": "pnpm run build:site && gh-pages -d site --add",
    "dev": "react-vant-cli dev",
    "test": "react-vant-cli test",
    "test:coverage": "open tests/coverage/index.html"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 8"
  ],
  "dependencies": {
    "@react-spring/web": "^9.3.0",
    "@react-vant/icons": "workspace:*",
    "@use-gesture/react": "^10.1.1",
    "@vant/popperjs": "^1.1.0",
    "clsx": "1.1.1",
    "rc-field-form": "^1.21.2",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@react-vant/cli": "workspace:*",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@vant/area-data": "1.1.3",
    "gh-pages": "3.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "4.6.3"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
